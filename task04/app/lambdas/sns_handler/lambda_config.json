{
  "version": "1.0",
  "name": "sns_handler",
  "func_name": "lambdas/sns_handler/index.handler",
  "resource_type": "lambda",
  "iam_role_name": "sns_handler-role",
  "runtime": "nodejs20.x",
  "memory": 128,
  "timeout": 100,
  "lambda_path": "lambdas/sns_handler",
  "dependencies": [
    {
      "resource_name": "arn:aws:sns:${region}:${account_id}:${resources_prefix}lambda_topic${resources_suffix}",
      "resource_type": "sns_topic"
    }
  ],
  "event_sources": [
    {
      "resource_name": "arn:aws:sns:${region}:${account_id}:${resources_prefix}lambda_topic${resources_suffix}",
      "target_topic": "arn:aws:sns:${region}:${account_id}:${resources_prefix}lambda_topic${resources_suffix}",
      "resource_type": "sns_topic_trigger",
      "batch_size" : 1,
      "batch_window": 1
    }
  ],
  "env_variables": {},
  "publish_version": true,
  "alias": "${lambdas_alias_name}",
  "url_config": {},
  "ephemeral_storage": 512
}
